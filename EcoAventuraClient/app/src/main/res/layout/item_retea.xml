<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#FFFFFF">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Retea (SSID) : "
        android:id="@+id/textView42"
        android:textColor="@color/colorTitle"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="12dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:textColor="@color/colorTitle"
        android:id="@+id/textView43"
        android:layout_alignTop="@+id/textView42"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView42"
        android:lines="1"
        android:ellipsize="end"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Adresa IP :"
        android:textColor="@color/colorTitle"
        android:id="@+id/textView44"
        android:layout_below="@+id/textView42"
        android:layout_alignStart="@+id/textView42"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_alignEnd="@+id/textView42" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:textColor="@color/colorTitle"
        android:id="@+id/textView45"
        android:layout_alignTop="@+id/textView44"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView42"
        android:lines="1"
        android:ellipsize="end"/>
</RelativeLayout>